{
  "expo": {
    "name": "Numeline",
    "slug": "eatsok",
    "scheme": "eatsafe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1F1F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eatsafe.app"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A1F1F"
      },
      "package": "com.eatsafe.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ]
    },
    "assetBundlePatterns": [
      "assets/**"
    ],
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Nous avons besoin d'accÃ©der Ã  votre appareil photo pour scanner les emballages."
        }
      ],
      [
        "expo-notifications",
        {
          "mode": "production"
        }
      ],
      "expo-localization",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "jsEngine": "hermes",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3d17326d-6c82-4b80-8f4d-9ea1e120fd85"
      }
    }
  }
}
